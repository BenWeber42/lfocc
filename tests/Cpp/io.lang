void printi(int i) {
   write(i);
   writeln();
}

void printf(float f) {
   writef(f);
   writeln();
}

void printb(boolean i) {
   if (i) {
      write(1);
   } else {
      write(0);
   }
   writeln();
}

boolean readb() {
   if (read() == 0) {
      return false;
   } else {
      return true;
   }
}

void main() {
   for (int i = read(); i > 0; i = i - 1) {
      printi(read());
   }

   for (int i = read(); i > 0; i = i - 1) {
      printf(readf());
   }

   for (int i = read(); i > 0; i = i - 1) {
      printb(readb());
   }
}
